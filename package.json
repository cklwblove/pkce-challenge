{
  "name": "pkce-challenge",
  "version": "3.0.0",
  "description": "Generate or verify a Proof Key for Code Exchange (PKCE) challenge pair",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "type": "module",
  "files": [
    "dist/"
  ],
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cklwblove/pkce-challenge.git"
  },
  "keywords": [
    "PKCE",
    "oauth2"
  ],
  "author": "crouchcd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cklwblove/pkce-challenge/issues"
  },
  "homepage": "https://github.com/cklwblove/pkce-challenge#readme",
  "dependencies": {
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^12.20.47",
    "tsup": "^6.5.0",
    "typescript": "^4.6.3"
  }
}
